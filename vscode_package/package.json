{
    "name": "sin-language",
    "description": "Syntax highlighting for the SIN programming language",
    "version": "0.1.0",
    "publisher": "lannonriley",
    "engines": {
        "vscode": "^1.40.0"
    },
    "displayName": "SIN Syntax Highlighting",
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "sin",
            "aliases": ["SIN", "sin"],
            "extensions": [".sin", ".sinh", ".sinl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "sin",
            "scopeName": "source.sin",
            "path": "./syntaxes/sin.tmLanguage.json"
        }]
    }
}
